<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" lazy="false" mutable="true" name="au.org.emii.portal.userdata.UserMap" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">userdata_id_seq</param>
      </generator>
    </id>
    <many-to-one class="au.org.emii.portal.userdata.UserPortal" column="userid" name="userid"/>
    <many-to-one cascade="all" class="au.org.emii.portal.value.BoundingBox" column="bBoxId" name="bBox"/>
    <many-to-one cascade="all" class="au.org.emii.portal.menu.MapLayer" column="currentBaseLayerId" name="currentBaseLayer"/>
    <property name="maximised" type="boolean">
      <column name="maximised"/>
    </property>
    <property name="currentNavigationTab" type="integer">
      <column name="currentNavigationTab"/>
    </property>
    <property name="currentView" type="integer">
      <column name="currentView"/>
    </property>
    <property name="mapname" type="string">
      <column name="mapname"/>
    </property>
  </class>
</hibernate-mapping>
